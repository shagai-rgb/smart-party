{
  "builds": [
    {
      "src": "apps/system-admin/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "apps/election/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "apps/national-audit/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "apps/political-party/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "apps/private-audit/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/system-admin/(.*)", "dest": "/index.html" },
    { "src": "/election/(.*)", "dest": "/index.html" },
    { "src": "/national-audit/(.*)", "dest": "/index.html" },
    { "src": "/political-party/(.*)", "dest": "/index.html" },
    { "src": "/private-audit/(.*)", "dest": "/index.html" }
  ]
}
